{
	"name": "horoscope-mobile-app",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"start": "node node_modules/react-native/local-cli/cli.js start",
		"test": "jest",
		"android": "tsc && react-native run-android"
	},
	"dependencies": {
		"cross-fetch": "^1.0.0",
		"immutable": "^3.8.2",
		"ms": "^2.0.0",
		"node-fetch": "^1.7.3",
		"react": "16.0.0-beta.5",
		"react-native": "0.49.3",
		"react-native-storage": "^0.2.2",
		"react-native-svg": "^5.4.2",
		"react-redux": "^5.0.6",
		"redux": "^3.7.2",
		"redux-loop-symbol-ponyfill": "^2.2.2",
		"redux-saga": "^0.16.0"
	},
	"devDependencies": {
		"@types/jest": "^21.1.5",
		"@types/ms": "^0.7.30",
		"@types/react": "^16.0.18",
		"@types/react-native": "^0.49.3",
		"@types/react-redux": "^5.0.12",
		"@types/react-test-renderer": "^16.0.0",
		"babel-jest": "21.2.0",
		"babel-preset-react-native": "4.0.0",
		"jest": "21.2.1",
		"react-test-renderer": "16.0.0-beta.5",
		"ts-jest": "^21.1.3",
		"typescript": "^2.5.3"
	},
	"jest": {
		"preset": "react-native",
		"moduleFileExtensions": [
			"ts",
			"tsx",
			"js"
		],
		"transform": {
			"^.+\\.(js)$": "<rootDir>/node_modules/babel-jest",
			"\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
		},
		"testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
		"testPathIgnorePatterns": [
			"\\.snap$",
			"<rootDir>/node_modules/",
			"<rootDir>/lib/"
		],
		"cacheDirectory": ".jest/cache"
	}
}
