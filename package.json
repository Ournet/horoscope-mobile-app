{
	"name": "horoscope-mobile-app",
	"version": "0.2.0",
	"private": true,
	"scripts": {
		"start": "node node_modules/react-native/local-cli/cli.js start",
		"test": "jest",
		"tsc": "copy src/**/*.json lib && tsc",
		"android": "npm run tsc && react-native run-android",
		"ios": "npm run tsc && react-native run-ios",
		"release": "cd android && ./gradlew assembleRelease"
	},
	"dependencies": {
		"cross-fetch": "^1.0.0",
		"immutable": "^3.8.2",
		"moment": "^2.19.1",
		"ms": "^2.0.0",
		"react": "16.0.0-beta.5",
		"react-native": "0.49.3",
		"react-native-linear-gradient": "^2.3.0",
		"react-native-onesignal": "^3.0.7",
		"react-native-storage": "^0.2.2",
		"react-native-svg": "^5.4.2",
		"react-native-vector-icons": "^4.4.2",
		"react-redux": "^5.0.6",
		"redux": "^3.7.2"
	},
	"devDependencies": {
		"@types/jest": "^21.1.5",
		"@types/ms": "^0.7.30",
		"@types/react": "^16.0.18",
		"@types/react-native": "^0.49.3",
		"@types/react-native-google-analytics-bridge": "^5.2.1",
		"@types/react-native-linear-gradient": "^2.3.0",
		"@types/react-native-vector-icons": "^4.4.2",
		"@types/react-redux": "^5.0.12",
		"@types/react-test-renderer": "^16.0.0",
		"babel-jest": "21.2.0",
		"babel-preset-react-native": "4.0.0",
		"copy": "^0.3.1",
		"jest": "21.2.1",
		"react-test-renderer": "16.0.0-beta.5",
		"ts-jest": "^21.1.3",
		"typescript": "^2.5.3"
	},
	"jest": {
		"preset": "react-native",
		"moduleFileExtensions": [
			"ts",
			"tsx",
			"js"
		],
		"transform": {
			"^.+\\.(js)$": "<rootDir>/node_modules/babel-jest",
			"\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
		},
		"testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
		"testPathIgnorePatterns": [
			"\\.snap$",
			"<rootDir>/node_modules/",
			"<rootDir>/lib/"
		],
		"cacheDirectory": ".jest/cache"
	}
}
